    "prettier": "prettier . --check",
    "watch:hubl": "hs watch routing-theme routing-theme",
    "upload:hubl": "hs upload routing-theme routing-theme",
    "deploy": "hs project upload routing-project"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
