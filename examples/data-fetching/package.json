    "watch:hubl": "hs watch data-fetching-theme data-fetching-theme",
    "upload:hubl": "hs upload data-fetching-theme",
    "deploy": "hs project upload data-fetching-project"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
